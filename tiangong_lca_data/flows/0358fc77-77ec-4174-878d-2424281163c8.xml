				<baseName xml:lang="zh">淀粉糖浆</baseName>
			</name>
			<common:synonyms xml:lang="en">Corn syrup,淀粉糖浆</common:synonyms>
			<common:synonyms xml:lang="zh">Corn syrup,淀粉糖浆</common:synonyms>
			<classificationInformation>
