			<name>
				<baseName xml:lang="en">Flower Pot</baseName>
				<baseName xml:lang="zh">陶制装饰性花盆</baseName>
			</name>
			<common:synonyms xml:lang="en">flowerpot; plant container; planter,陶制装饰性花盆</common:synonyms>
			<common:synonyms xml:lang="zh">flowerpot; plant container; planter,陶制装饰性花盆</common:synonyms>
			<classificationInformation>
