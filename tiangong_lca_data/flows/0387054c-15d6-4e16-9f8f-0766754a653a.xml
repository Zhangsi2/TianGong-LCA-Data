			<name>
				<baseName xml:lang="en">Emulsion-polymerized styrene butadiene rubber (E-SBR)</baseName>
				<baseName xml:lang="zh">丁苯橡胶</baseName>
			</name>
			<common:synonyms xml:lang="en">E-SBR; Emulsion Polymerized Styrene-butadiene Rubber,丁苯橡胶</common:synonyms>
			<common:synonyms xml:lang="zh">E-SBR; Emulsion Polymerized Styrene-butadiene Rubber,丁苯橡胶</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
					<common:class level="0">Materials production</common:class>
					<common:class level="1">Organic chemicals</common:class>
