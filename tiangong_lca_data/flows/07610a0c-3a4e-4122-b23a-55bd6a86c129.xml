			<name>
				<baseName xml:lang="en">Thermos Bottle</baseName>
				<baseName xml:lang="zh">玻璃保温瓶胆</baseName>
			</name>
			<common:synonyms xml:lang="en">Vacuum Flask; Beverage Bottle,玻璃保温瓶胆</common:synonyms>
			<common:synonyms xml:lang="zh">Vacuum Flask; Beverage Bottle,玻璃保温瓶胆</common:synonyms>
