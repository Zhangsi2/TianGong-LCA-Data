				<baseName xml:lang="en">Cement</baseName>
				<baseName xml:lang="zh">水泥</baseName>
			</name>
			<common:synonyms xml:lang="en">Concrete; Mortar</common:synonyms>
			<common:synonyms xml:lang="zh">Concrete; Mortar</common:synonyms>
