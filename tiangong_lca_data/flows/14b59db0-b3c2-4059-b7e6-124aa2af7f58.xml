			<name>
				<baseName xml:lang="en">Ethylene glycol</baseName>
				<baseName xml:lang="zh">乙二醇</baseName>
			</name>
			<common:synonyms xml:lang="en">Ethylene; Glycol; Diol</common:synonyms>
			<common:synonyms xml:lang="zh">Ethylene; Glycol; Diol</common:synonyms>
			<classificationInformation>
