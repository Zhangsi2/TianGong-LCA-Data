				<baseName xml:lang="en">Deionized Water</baseName>
				<baseName xml:lang="zh">去离子水</baseName>
			</name>
			<common:synonyms xml:lang="en">DI water; Demineralized water</common:synonyms>
			<common:synonyms xml:lang="zh">DI water; Demineralized water</common:synonyms>
			<classificationInformation>
