				<baseName xml:lang="en">Vermicelli</baseName>
				<baseName xml:lang="zh">挂面</baseName>
			</name>
			<common:synonyms xml:lang="en">Misua; Rice vermicelli; Corn vermicelli; Falooda sev,挂面</common:synonyms>
			<common:synonyms xml:lang="zh">Misua; Rice vermicelli; Corn vermicelli; Falooda sev,挂面</common:synonyms>
