			<name>
				<baseName xml:lang="en">UV Light</baseName>
				<baseName xml:lang="zh">上光油（UV）</baseName>
			</name>
			<common:synonyms xml:lang="en">Ultraviolet; UV,上光油（UV）</common:synonyms>
			<common:synonyms xml:lang="zh">Ultraviolet; UV,上光油（UV）</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
					<common:class level="0">Materials production</common:class>
