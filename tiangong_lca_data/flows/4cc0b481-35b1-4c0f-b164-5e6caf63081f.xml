			<name>
				<baseName xml:lang="en">Canned Mandarin Oranges</baseName>
				<baseName xml:lang="zh">橘子罐头</baseName>
			</name>
			<common:synonyms xml:lang="en">Canned mandarin; Canned Satsuma,橘子罐头</common:synonyms>
			<classificationInformation>
