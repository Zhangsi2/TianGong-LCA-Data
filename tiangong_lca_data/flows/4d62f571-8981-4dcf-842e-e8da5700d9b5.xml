			<name>
				<baseName xml:lang="en">Saw</baseName>
				<baseName xml:lang="zh">锯材、木片、单板</baseName>
			</name>
			<common:synonyms xml:lang="en">Cutting tool; Woodworking tool,锯材、木片、单板</common:synonyms>
			<common:synonyms xml:lang="zh">Cutting tool; Woodworking tool,锯材、木片、单板</common:synonyms>
