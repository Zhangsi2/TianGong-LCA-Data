			<name>
				<baseName xml:lang="en">Tungsten Powder</baseName>
				<baseName xml:lang="zh">钨粉</baseName>
			</name>
			<common:synonyms xml:lang="en">W powder; Granulated tungsten powders,钨粉</common:synonyms>
			<common:synonyms xml:lang="zh">W powder; Granulated tungsten powders,钨粉</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
