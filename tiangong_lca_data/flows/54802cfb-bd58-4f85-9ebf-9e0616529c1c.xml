				<baseName xml:lang="en">Polypropylene</baseName>
				<baseName xml:lang="zh">聚丙烯</baseName>
			</name>
			<common:synonyms xml:lang="en">PP,聚丙烯</common:synonyms>
			<classificationInformation>
