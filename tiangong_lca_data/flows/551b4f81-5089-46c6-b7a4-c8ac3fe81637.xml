			<name>
				<baseName xml:lang="en">Vanadium Titano-Magnetite</baseName>
				<baseName xml:lang="zh">钒钛磁铁矿石</baseName>
			</name>
			<common:synonyms xml:lang="en">VTM,钒钛磁铁矿石</common:synonyms>
			<common:synonyms xml:lang="zh">VTM,钒钛磁铁矿石</common:synonyms>
			<classificationInformation>
