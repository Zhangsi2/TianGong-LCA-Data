				<baseName xml:lang="en">Baijiu</baseName>
				<baseName xml:lang="zh">清香型白酒（原酒）</baseName>
			</name>
			<common:synonyms xml:lang="en">Shaojiu,清香型白酒（原酒）</common:synonyms>
			<classificationInformation>
