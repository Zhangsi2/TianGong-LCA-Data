			<common:synonyms xml:lang="en">Alcoholic beverage; Malted cereal grain drink; Brewed drink,啤酒</common:synonyms>
			<common:synonyms xml:lang="zh">Alcoholic beverage; Malted cereal grain drink; Brewed drink,啤酒</common:synonyms>
