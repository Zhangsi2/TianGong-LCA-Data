			<name>
				<baseName xml:lang="en">Surimi</baseName>
				<baseName xml:lang="zh">鱼糜制品</baseName>
			</name>
			<common:synonyms xml:lang="en">Imitation crab; Processed seafood product; Ground meat; Minced meat,鱼糜制品</common:synonyms>
			<common:synonyms xml:lang="zh">Imitation crab; Processed seafood product; Ground meat; Minced meat,鱼糜制品</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
