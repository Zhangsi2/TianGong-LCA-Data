			<name>
				<baseName xml:lang="en">Garment Wash</baseName>
				<baseName xml:lang="zh">水洗加工成衣服装</baseName>
			</name>
			<common:synonyms xml:lang="en">Garments Washing,水洗加工成衣服装</common:synonyms>
			<common:synonyms xml:lang="zh">Garments Washing,水洗加工成衣服装</common:synonyms>
