			<common:synonyms xml:lang="en">ED copper foil; Electrolytic copper,电积铜</common:synonyms>
			<common:synonyms xml:lang="zh">ED copper foil; Electrolytic copper,电积铜</common:synonyms>
			<classificationInformation>
