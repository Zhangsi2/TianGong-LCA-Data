			<name>
				<baseName xml:lang="en">Hot-dip galvanizing</baseName>
				<baseName xml:lang="zh">热浸锌件</baseName>
			</name>
			<common:synonyms xml:lang="en">HDG; Hot-dip galvanized steel strip; Galvanized iron,热浸锌件</common:synonyms>
			<classificationInformation>
				<common:classification>
					<common:category level="0">Materials production</common:category>
