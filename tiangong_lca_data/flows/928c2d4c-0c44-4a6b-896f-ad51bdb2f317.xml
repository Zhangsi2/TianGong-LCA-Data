				<baseName xml:lang="zh">卫生纸</baseName>
			</name>
			<common:synonyms xml:lang="en">Toilet Tissue; Bathroom Tissue,卫生纸</common:synonyms>
			<common:synonyms xml:lang="zh">Toilet Tissue; Bathroom Tissue,卫生纸</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
					<common:class level="0">Materials production</common:class>
