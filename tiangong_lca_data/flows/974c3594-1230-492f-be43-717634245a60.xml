			<name>
				<baseName xml:lang="en">Silver Foil</baseName>
				<baseName xml:lang="zh">银箔材</baseName>
			</name>
			<common:synonyms xml:lang="en">Varak; Vark; Varakh,银箔材</common:synonyms>
			<common:synonyms xml:lang="zh">Varak; Vark; Varakh,银箔材</common:synonyms>
			<classificationInformation>
