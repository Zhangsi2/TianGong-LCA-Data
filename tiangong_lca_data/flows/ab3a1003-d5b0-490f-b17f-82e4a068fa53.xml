			<name>
				<baseName xml:lang="en"> Sodium carbonate</baseName>
				<baseName xml:lang="zh"> 碳酸钠</baseName>
			</name>
			<common:synonyms xml:lang="en">washing soda; soda ash; soda crystals</common:synonyms>
			<common:synonyms xml:lang="zh">washing soda; soda ash; soda crystals</common:synonyms>
			<classificationInformation>
