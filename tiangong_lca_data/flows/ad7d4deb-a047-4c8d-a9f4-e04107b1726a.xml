			<common:synonyms xml:lang="en">Pine Plus; All-Purpose Cleaner,KDS-101清洗剂</common:synonyms>
			<common:synonyms xml:lang="zh">Pine Plus; All-Purpose Cleaner,KDS-101清洗剂</common:synonyms>
