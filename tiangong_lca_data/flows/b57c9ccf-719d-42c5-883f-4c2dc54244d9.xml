			<name>
				<baseName xml:lang="en">Yellow Phosphorus</baseName>
				<baseName xml:lang="zh">黄磷</baseName>
			</name>
			<common:synonyms xml:lang="en">Phosphorus; Phosphorus tetramer; White phosphorus,黄磷</common:synonyms>
			<common:synonyms xml:lang="zh">Phosphorus; Phosphorus tetramer; White phosphorus,黄磷</common:synonyms>
			<classificationInformation>
