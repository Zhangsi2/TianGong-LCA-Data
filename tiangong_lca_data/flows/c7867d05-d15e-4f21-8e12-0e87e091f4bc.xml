			<name>
				<baseName xml:lang="en">Phenolic Baking Electro-Deposition Primer</baseName>
				<baseName xml:lang="zh">电泳底漆</baseName>
			</name>
			<common:synonyms xml:lang="en">Phenolic resins; Resole,电泳底漆</common:synonyms>
			<common:synonyms xml:lang="zh">Phenolic resins; Resole,电泳底漆</common:synonyms>
