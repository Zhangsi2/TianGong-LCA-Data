			<common:synonyms xml:lang="en">Quartz gold ore; Auriferous granite,金矿石</common:synonyms>
			<common:synonyms xml:lang="zh">Quartz gold ore; Auriferous granite,金矿石</common:synonyms>
			<classificationInformation>
