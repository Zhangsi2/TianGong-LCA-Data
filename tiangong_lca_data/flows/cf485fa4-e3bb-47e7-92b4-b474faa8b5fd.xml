			<name>
				<baseName xml:lang="en">Medical Absorbent Cotton</baseName>
				<baseName xml:lang="zh">医用纱布/脱脂棉</baseName>
			</name>
			<common:synonyms xml:lang="en">Absorbent cotton; Medical cotton ball,医用纱布/脱脂棉</common:synonyms>
			<common:synonyms xml:lang="zh">Absorbent cotton; Medical cotton ball,医用纱布/脱脂棉</common:synonyms>
