			<name>
				<baseName xml:lang="en">Dicamba</baseName>
				<baseName xml:lang="zh">麦草畏</baseName>
			</name>
			<common:synonyms xml:lang="en">3,6-Dichloro-2-methoxybenzoic acid; Banvel; Clarity; Vanquish; Oracle; Diablo,麦草畏</common:synonyms>
			<common:synonyms xml:lang="zh">3,6-Dichloro-2-methoxybenzoic acid; Banvel; Clarity; Vanquish; Oracle; Diablo,麦草畏</common:synonyms>
			<classificationInformation>
