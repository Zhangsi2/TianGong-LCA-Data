			<name>
				<baseName xml:lang="en">White Granulated Sugar</baseName>
				<baseName xml:lang="zh">白砂糖、绵白糖</baseName>
			</name>
			<common:synonyms xml:lang="en">Granulated sugar; Table sugar,白砂糖、绵白糖</common:synonyms>
			<classificationInformation>
				<common:classification>
					<common:category level="0">Materials production</common:category>
