			<name>
				<baseName xml:lang="en">Aluminium Profile</baseName>
				<baseName xml:lang="zh">铝型材</baseName>
			</name>
			<common:synonyms xml:lang="en">Aluminum extrusions,铝型材</common:synonyms>
			<common:synonyms xml:lang="zh">Aluminum extrusions,铝型材</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
