				<baseName xml:lang="en">Thick Chilli Sauce</baseName>
				<baseName xml:lang="zh">香辣酱</baseName>
			</name>
			<common:synonyms xml:lang="en">Tomato-based chili sauce; Fresh tomato chili sauce,香辣酱</common:synonyms>
			<classificationInformation>
				<common:classification>
					<common:category level="0">Materials production</common:category>
					<common:category level="1">Food and renewable raw materials</common:category>
