			<name>
				<baseName xml:lang="en">Sheepskin</baseName>
				<baseName xml:lang="zh">羊皮/细杂皮</baseName>
			</name>
			<common:synonyms xml:lang="en">Lambskin; Pelt,羊皮/细杂皮</common:synonyms>
			<common:synonyms xml:lang="zh">Lambskin; Pelt,羊皮/细杂皮</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
					<common:class level="0">Materials production</common:class>
