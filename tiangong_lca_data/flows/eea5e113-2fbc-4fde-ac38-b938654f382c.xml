			<name>
				<baseName xml:lang="en">Natural Silk</baseName>
				<baseName xml:lang="zh">蚕丝、印染丝织物</baseName>
			</name>
			<common:synonyms xml:lang="en">Mulberry Silk,蚕丝、印染丝织物</common:synonyms>
			<common:synonyms xml:lang="zh">Mulberry Silk,蚕丝、印染丝织物</common:synonyms>
