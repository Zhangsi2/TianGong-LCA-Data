				<baseName xml:lang="zh">水泥研磨;水泥, 普通硅酸盐水泥</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Materials production</common:class>
					<common:class level="1">Other mineralic materials</common:class>
				</common:classification>
			</classificationInformation>
			<common:generalComment xml:lang="en">Ordinary Portland cement (OPC)</common:generalComment>
			<common:generalComment xml:lang="zh">普通硅酸盐水泥（Ordinary portland cement，OPC）</common:generalComment>
