	<processInformation>
		<dataSetInformation>
			<common:UUID>756442fe-5398-4324-8aa8-ff4bba568a22</common:UUID>
			<name>
				<baseName xml:lang="en"> Soil and groundwater remediation;Ex-situ;Soil washing;Site foundation</baseName>
				<baseName xml:lang="zh">土壤和地下水修复;原位;土壤冲洗;场地基础设施建设</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Other Services</common:class>
					<common:class level="1">Repair and maintenance</common:class>
				</common:classification>
			</classificationInformation>
			<common:generalComment xml:lang="en">include excavation of soil, transportation of soil and washing equipment, installation of washing equipment, and ground foundation</common:generalComment>
			<common:generalComment xml:lang="zh">包括土壤开挖、土壤和冲洗设备运输、冲洗设备安装和地基</common:generalComment>
		</dataSetInformation>
		<quantitativeReference type="Reference flow(s)">
			<referenceToReferenceFlow>2</referenceToReferenceFlow>
		</quantitativeReference>
