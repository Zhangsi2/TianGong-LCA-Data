			<name>
				<baseName xml:lang="en">coal washing| Separation of coal, gangue and clean coal washing</baseName>
				<baseName xml:lang="zh">煤炭洗选|将煤炭、矸石、精煤洗选分离</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Materials production</common:class>
