			<name>
				<baseName xml:lang="en">Soil Remediation;In-situ Remediation;Bioventilation;Exhaust Gas Treatment</baseName>
				<baseName xml:lang="zh">土壤修复;原位修复;生物通风;废气得到处理</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Other Services</common:class>
					<common:class level="1">Repair and maintenance</common:class>
				</common:classification>
			</classificationInformation>
			<common:generalComment xml:lang="en">Treatment of extracted exhaust gases</common:generalComment>
			<common:generalComment xml:lang="zh">对抽提上来的废气进行处理</common:generalComment>
		</dataSetInformation>
		<quantitativeReference type="Reference flow(s)">
			<referenceToReferenceFlow>6</referenceToReferenceFlow>
		</quantitativeReference>
